CREATE DATABASE HOBBYES;

use HOBBYES;

CREATE TABLE PEOPLE 
(ID INT(10) PRIMARY KEY NOT NULL  AUTO_INCREMENT,
FNAME VARCHAR(50),
E_MAIL VARCHAR(50)
);

desc PEOPLE;

SHOW TABLEs;

CREATE TABLE PET
(ID INT(10) PRIMARY KEY AUTO_INCREMENT,
Fname VARCHAR(50),
P_ID int(10),
foreign key (P_ID) REFERENCES PEOPLE (ID)
 );
DESC PET
